{
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "no-unused-vars": 0,
    "operator-linebreak": ["error", "before"],
    "linebreak-style": 0,
    "camelcase": 0,
    "max-params": 0,
    "space-before-function-paren": ["error", "never"],
    "space-before-blocks": ["error", "never"]
  }
}